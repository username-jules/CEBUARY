                              </ImageView>
                           </graphic>
                        </Button>
                        <Button fx:id="dictionaryButton" alignment="TOP_LEFT" layoutX="14.0" layoutY="203.0" mnemonicParsing="false" onAction="#showDictionary" prefHeight="50.0" prefWidth="265.0" style="-fx-background-color: transparent; -fx-text-fill: black;" text="Dictionary" AnchorPane.leftAnchor="14.0">
