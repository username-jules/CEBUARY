                        <Button fx:id="chabaFlashButton" alignment="TOP_LEFT" layoutX="14.0" layoutY="363.0" mnemonicParsing="false" onAction="#showChabaFlash" prefHeight="50.0" prefWidth="265.0" style="-fx-background-color: transparent; -fx-text-fill: black;" text="Chabaflash" AnchorPane.leftAnchor="14.0">
